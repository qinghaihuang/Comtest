<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="admin" catalog="comtest" class="entity.Admin">
		<primary-key>
			<generator class="uuid.hex"></generator>
		</primary-key>
	</table>
	<table name="login" catalog="comtest" class="entity.Login">
		<primary-key>
			<generator class="uuid.hex"></generator>
		</primary-key>
	</table>
	<table name="question" catalog="comtest" class="entity.Question">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="person" catalog="stms" class="entity.Person">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="student" catalog="comtest" class="entity.Student">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="teacher" catalog="comtest" class="entity.Teacher">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="single_choice" catalog="comtest"
		class="entity.Single_choice">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="test" catalog="comtest" class="entity.Test">
	<primary-key>
		<generator class="identity"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>